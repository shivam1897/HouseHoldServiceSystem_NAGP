{
  "Routes": [
    {
      "ServiceName": "Service.Admin",
      "DownstreamPathTemplate": "/api/admin/adminservice/",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/admin",
      "UpstreamHttpMethod": [ "GET" ],
      "QoSOptions": {
        "ExceptionsAllowedBeforeBreaking": 1,
        "DurationOfBreak": 1000,
        "TimeoutValue": 10000
      }
    },
    {
      "ServiceName": "Service.Admin",
      "DownstreamPathTemplate": "/api/admin/adminservice/{id}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/admin/{id}",
      "UpstreamHttpMethod": [ "GET" ],
      "QoSOptions": {
        "ExceptionsAllowedBeforeBreaking": 2,
        "DurationOfBreak": 1000,
        "TimeoutValue": 10000
      }
    },
    {
      "ServiceName": "Service.Provider",
      "DownstreamPathTemplate": "/provider/randomTimeOut",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/provider/randomTimeOut",
      "UpstreamHttpMethod": [ "GET" ],
      "QoSOptions": {
        "ExceptionsAllowedBeforeBreaking": 1,
        "DurationOfBreak": 1000,
        "TimeoutValue": 5000
      }
    }
  ],
  "GlobalConfiguration": {
    "Host": "consul",
    "Port": "8500",
    "Type": "Consul"
  }

}
